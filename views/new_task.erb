<form action="/tasks/new" method="POST">
  Name: <input type="text" name="name"><br>
  Descriptions: <input type="text" name="descriptions"><br>
  Movie:
  <select name="movie_id">
    <% @movies.each do |movie| %>
    <option value="<%= movie["id"] %>"><%= movie["name"] %>
    </option>
    <% end %>
  </select><br>


  Contact: <input type="text" name="contact_id"><br>
  <button>Creat New Task</button>
</form>